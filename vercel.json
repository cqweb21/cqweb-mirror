{
  "headers": [
    {
      "source": "/gb/(.*)",
      "headers": [
        { "key": "Content-Type", "value": "text/html; charset=gb18030" }
      ]
    },
    {
      "source": "/big5/(.*)",
      "headers": [
        { "key": "Content-Type", "value": "text/html; charset=big5" }
      ]
    },
    {
      "source": "/(.*)\\.(html|htm|shtml)",
      "headers": [
        { "key": "Content-Type", "value": "text/html; charset=utf-8" }
      ]
    }
  ]
}
