{
  "routes": [
    { "src": "^/gb/?$", "dest": "/gb/index.html" },
    { "src": "^/big5/?$", "dest": "/big5/index.html" }
  ],
  "headers": [
    {
      "source": "/gb/:path*",
      "headers": [
        { "key": "Content-Type", "value": "text/html; charset=gb18030" }
      ]
    },
    {
      "source": "/big5/:path*",
      "headers": [
        { "key": "Content-Type", "value": "text/html; charset=big5" }
      ]
    }
  ]
}
